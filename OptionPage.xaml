             x:Class="Salon_Infrumusetare2.OptionPage"
             Title="OptionPage">
    <VerticalStackLayout Spacing="25" Padding="30">
        <Button x:Name="btnSpecialisti" Text="Specialiști" Clicked="OnSpecialistiClicked" />
        <Button x:Name="btnServicii" Text="Servicii" Clicked="OnServiciiClicked" />
        <Button x:Name="btnRecenzii" Text="Recenzii" Clicked="OnRecenziiClicked" />
        <Button x:Name="btnClienti" Text="Clienți" Clicked="OnClientiClicked" />
    </VerticalStackLayout>
</ContentPage>